<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#aa000000" />
            <stroke android:width="1.0px" android:color="@color/yellow_pro" />
            <corners android:radius="3.0px" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/yellow_pro" />
                <stroke android:width="0.0px" android:color="@color/yellow_pro" />
                <corners android:radius="3.0px" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="#ffffffff" />
                <stroke android:width="1.0px" android:color="#ffffffff" />
                <corners android:radius="3.0px" />
                <gradient android:startColor="#aa000000" android:endColor="#ffffffff" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>